<div id="<%= dom_id glass_price %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Glass type:</strong>
    <%= glass_price.glass_type %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Thickness:</strong>
    <%= glass_price.thickness %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Color:</strong>
    <%= glass_price.color %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Price:</strong>
    <%= glass_price.price %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Price m2:</strong>
    <%= glass_price.price_m2 %>
  </p>
</div>
